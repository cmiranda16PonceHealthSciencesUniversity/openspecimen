<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.common.domain.PrintRules" table="OS_PRINT_RULES">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_PRINT_RULES_SEQ</param>
      </generator>
    </id>

    <many-to-one name="collectionProtocol" column="CP_ID"/>

    <many-to-one name="visitSite" column="VISIT_SITE_ID"/>

    <property name="specimenClass"  column="SPECIMEN_CLASS"/>

    <property name="specimenType" column="SPECIMEN_TYPE"/>

    <many-to-one name="user" column="USER_ID"/>

    <property name="ipRange" column="IP_RANGE"/>

    <property name="labelType" column="LABEL_TYPE"/>

    <property name="labelDesign" column="LABEL_DESIGN"/>

    <property name="printer" column="PRINTER"/>

    <property name="cmdFileDir" column="CMD_FILE_DIR"/>

    <property name="cmdFileFmt" column="CMD_FILE_FMT"/>

    <property name="lineage" column="LINEAGE"/>

    <set name="labelTokens" table="OS_PRINT_LABEL_TOKENS" inverse="true" cascade="all-delete-orphan">
      <key column="RULE_ID"/>
      <one-to-many class="com.krishagni.catissueplus.core.common.domain.PrintLabelTokens"/>
    </set>
  </class>
</hibernate-mapping>
